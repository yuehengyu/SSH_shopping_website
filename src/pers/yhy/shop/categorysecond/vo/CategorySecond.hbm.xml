<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
    <hibernate-mapping>
	<class name="pers.yhy.shop.categorysecond.vo.CategorySecond" table="categorysecond">
		<id name="csid">
			<generator class="native" />
		</id>
		<property name="csname" />
		
		<many-to-one name="category" lazy="false" class="pers.yhy.shop.category.vo.Category" column="cid"></many-to-one>
		<set name="products">
			<key column="csid" />
			<one-to-many class="pers.yhy.shop.product.vo.Product"/>
		</set>
	</class>
</hibernate-mapping>